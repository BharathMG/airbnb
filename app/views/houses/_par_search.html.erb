<% form_for @search do |f| %>
    <fieldset>
        <legend>Search By</legend>

        <% form_for @search do |houses| %>
        <%= houses.hidden_field :city_like, :id => "city2" %>
            <%= houses.label :r_type %><br />
            <%= houses.text_field :r_type_greater_than_or_equal_to %><br />
            <%= houses.label :rent %>
            <%= houses.text_field :rent_less_than_or_equal_to %><br />
            <%= houses.label :Bedrooms %>
            <%= houses.text_field :B_rooms_equals %><br />
            <%= houses.label :Minimum_stay %>
            <%= houses.text_field :min_stay_greater_than_or_equal_to %><br />
                <%= f.submit "Submit" %>
            <br />
            <% end %>

<% end %>
